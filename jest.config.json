{
    "collectCoverage": true,
    "verbose": true,
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "collectCoverage": true,
    "coverageReporters": ["json-summary", "text"],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.js",
      "!src/init.js",
      "!src/metrics.js",
      "!src/logging.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
      
}
